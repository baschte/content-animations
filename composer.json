{
	"name": "baschte/content-animations",
	"type": "typo3-cms-extension",
	"description": "TYPO3 CMS extension to have some fun and animate your content elements",
	"homepage": "https://www.raphael-gmbh.de",
	"keywords": ["typo3", "TYPO3 CMS", "tools", "animations", "frontend", "raphael"],
	"version": "2.3.3",
	"license": [
		"GPL-3.0-or-later"
	],
	"authors": [
		{
			"name": "Sebastian Richter",
			"role": "Developer"
		}
	],
	"require": {
		"typo3/cms-core": "^10.4 || ^11.5 || ^12.0"
	},
	"autoload": {
		"psr-4": {
			"Baschte\\ContentAnimations\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Baschte\\ContentAnimations\\Tests\\": "Tests"
		}
	},
	"extra": {
        "typo3/cms": {
            "extension-key": "content_animations"
        }
    }
}
